<functionlist
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder
                        https://www.mmbase.org/xmlns/builder.xsd"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    >

  <function name="thumbnail">
    <class>org.mmbase.streams.thumbnails.ThumbNailFunction</class>
  </function>

  <function name="gui">
    <class>org.mmbase.streams.thumbnails.GuiFunction</class>
  </function>

  <function key="{NODE.number}: {NODE.url}" name="entrylistgui">
    <class>org.mmbase.util.functions.PatternNodeFunctionProvider</class>
  </function>
</functionlist>
