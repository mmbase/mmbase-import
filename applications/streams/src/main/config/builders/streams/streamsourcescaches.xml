<builder
    xmlns="http://www.mmbase.org/xmlns/builder"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.mmbase.org/xmlns/builder
                        https://www.mmbase.org/xmlns/builder.xsd"
    xmlns:xi="http://www.w3.org/2001/XInclude"
    name="streamsourcescaches" maintainer="mmbase.org" version="1" extends="mediasources">

  <names>
    <singular xml:lang="en">Streamcache</singular>
    <singular xml:lang="nl">Streamcache</singular>
    <plural xml:lang="en">Streamcaches</plural>
    <plural xml:lang="nl">Streamcaches</plural>
  </names>
  <descriptions>
    <description xml:lang="en">
      Streamsourcescaches are the results of transcoding a source stream into different streams.
    </description>
    <description xml:lang="nl">
      Streamsourcescaches bevatten de resultaten van het transcoderen van een stream in nieuwe streams.
    </description>
  </descriptions>

  <properties>
  </properties>

  <xi:include href="streamsourcescaches.fields.xml" />
  <xi:include href="https://www.mmbase.org/builders/media/mediasources.virtual.fields.xml" />

  <functionlist>
    <function name="file">
      <class>org.mmbase.streams.FileFunction</class>
    </function>
  </functionlist>
</builder>
